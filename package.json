{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "main": "./main/main.js",
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.18.6",
    "axios": "^0.25.0",
    "custom-electron-titlebar": "^4.1.0",
    "electron-updater": "^4.6.5",
    "frameless-titlebar": "^2.1.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.34.14",
    "react-redux": "^7.2.6",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^4.0.0",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1"
  },
  "scripts": {
    "start": "TAILWIND_MODE=watch craco start",
    "electron:start": "NODE_ENV=development TAILWIND_MODE=watch electron .",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "env": {
      "amd": true,
      "es6": true,
      "browser": true,
      "node": false
    },
    "globals": {
      "process": true
    },
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-duplicate-imports": "error",
      "quotes": [
        2,
        "single"
      ],
      "no-const-assign": 2,
      "no-this-before-super": 2,
      "react/jsx-no-undef": 1,
      "react/no-multi-comp": 2,
      "react/no-deprecated": 1,
      "no-mixed-spaces-and-tabs": 2,
      "no-func-assign": 2,
      "no-empty": 2,
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^6.4.3",
    "autoprefixer": "^9.8.8",
    "babel-plugin-import": "^1.13.3",
    "craco-less": "^2.0.0",
    "electron": "^17.0.0",
    "electron-builder": "^22.14.13",
    "postcss": "^7.0.39",
    "redux-logger": "^3.0.6",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17"
  }
}
